{
  "jsonid": "input_profile",
  "controller_type": "mycontroller",
  "input_bindingui_mode": "controller_handed",
  "input_bindingui_left": {
    "image": "{hellion}/icons/sample_controller.svg"
  },
  "input_bindingui_right": {
    "image": "{hellion}/icons/sample_controller.svg"
  },
  "should_show_binding_errors": true,
  "input_source": {
    "/input/a": {
      "binding_image_point": [ 80, 60 ],
      "type": "button",
      "touch": true,
      "order": 1
    },
    "/input/trigger": {
      "binding_image_point": [ 250, 60 ],
      "type": "trigger",
      "order": 2
    },
    "/input/joystick": {
      "type": "joystick",
      "click": false,
      "touch": false,
      "order": 4
    },
    "default_bindings": []
  }
}